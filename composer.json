{
  "require": {
    "yiisoft/yii2": "~2.0.10",
    "yiisoft/yii2-bootstrap": "~2.0.6",
    "jamband/ripple": "^0.3",
    "creocoder/yii2-taggable": "^2.0",
    "hashids/hashids": "^2.0"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.0.7",
    "yiisoft/yii2-gii": "~2.0.5",
    "vlucas/phpdotenv": "^2.2",
    "codeception/codeception": "^2.2"
  },
  "provide": {
    "bower-asset/jquery": "*",
    "bower-asset/jquery.inputmask": "*",
    "bower-asset/bootstrap": "*",
    "bower-asset/punycode": "*",
    "bower-asset/yii2-pjax": "*",
    "bower-asset/typeahead.js": "*"
  },
  "scripts": {
    "clean": [
      "rm -rf node_modules runtime/* vendor web/assets/*"
    ],
    "show-asset-version": [
      "composer show -t | grep -o 'bower-asset.*' | sort | uniq"
    ]
  }
}
